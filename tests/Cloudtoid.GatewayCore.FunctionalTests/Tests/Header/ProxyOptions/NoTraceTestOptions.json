{
  "routes": {
    "/api/": {
      "proxy": {
        "to": "https://localhost:$upstream-port/header/",
        "upstreamRequest": {
          "headers": {
            "skipCorrelationId": true,
            "skipCallId": true
          }
        },
        "downstreamResponse": {
          "headers": {
            "addCorrelationId": true,
            "addCallId": true
          }
        }
      }
    }
  }
}
